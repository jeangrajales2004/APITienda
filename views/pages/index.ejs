<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

    <header>
        <%- include('../partials/header'); %>
    </header>

    <main>
        <div class="jumbotron">
            <h1>Top Productos</h1>
            <ul>
                <% listaProducto.forEach(function(producto) { %>
                    <li>
                        <strong><%= producto.nombre %></strong>
                        <%= producto.referencia %> <br>
                        <%= producto.precio %> <br>
                    </li>
                <% }); %>
            </ul>
        </div>
    </main>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>
</html>